/////////////////////////////
// Button control:
/////////////////////////////

void setup() {
  Serial.begin(9600);
  pinMode(buttonPin, INPUT);
  matrix.begin();
  matrix.setBrightness(brightness);
  matrix.fillScreen(0);
  matrix.show();
  theTimer = AnimateTimer.setInterval(60, snes_box);
  functionPtrs[0] = snes_box;
  animationSpeeds[0] = 60;
  functionPtrs[1] = screamFace;
  animationSpeeds[1] = 45;
  functionPtrs[2] = pumpkin;
  animationSpeeds[2] = 25;
  functionPtrs[3] = laughingEmoji;
  animationSpeeds[3] = 20;
}


void loop() {
  // read the state of the pushbutton value:
  buttonState = digitalRead(buttonPin);

  AnimateTimer.run();
  matrix.show();

  if ((millis() - lastDebounceTime) > debounceDelay) {

    
    // check if the pushbutton is pressed. If it is, the buttonState is HIGH:
    if (buttonState == HIGH) {
      nextState();
      lastDebounceTime = millis();
    }
  }

}

void nextState(){
  matrix.fillScreen(0);
  matrix.show();
  AnimateTimer.deleteTimer(theTimer);
  int value = nextIndex();
  Serial.print("setting speed to:");
  Serial.print(animationSpeeds[value]);
  AnimateTimer.setInterval(animationSpeeds[value], (*functionPtrs[value]));
}

int nextIndex(){
  if (currentIndex == 3){
    currentIndex = 0;
  } else {
    currentIndex += 1; 
  }
  return currentIndex;
}

